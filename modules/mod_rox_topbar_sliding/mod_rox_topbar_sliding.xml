<?xml version="1.0" encoding="UTF-8"?><extension type="module" version="2.5.0" client="site" method="upgrade">	<name>Rox Topbar Sliding</name>	<creationDate>January 2013</creationDate>	<author>ThemeRox</author>	<authorEmail>support@ThemeRox.com</authorEmail>	<authorUrl>http://www.ThemeRox.com</authorUrl>	<copyright>Copyright (C) 2010 - 2012 ThemeRox All rights reserved.</copyright>    <license>Copyrighted Commercial</license>	<version>1.0</version>		<description>Rox Topbar Sliding joomla Module</description>	<languages>		<language tag="en-GB">language/en-GB/en-GB.mod_rox_topbar_sliding.ini		</language>	</languages>	<files>		<folder>assets</folder>		<folder>elements</folder>		<folder>tmpl</folder>		<filename module="mod_rox_topbar_sliding">mod_rox_topbar_sliding.php</filename>		<filename>mod_rox_topbar_sliding.xml</filename>		<filename>index.html</filename>	</files>	<config>	    		<fields name="params" addfieldpath="/modules/mod_rox_topbar_sliding/elements">		<fieldset name="Option" label="Option">		        <field					name="load_jquery"					type="radio"					default="0"					label="LOAD_JQUERY"					description="LOAD_JQUERY_DESC">					<option						value="0">JNo</option>					<option						value="1">JYes</option>				</field>				<field					name="always_open"					type="radio"					default="0"					label="ALWAYS_OPEN"					description="ALWAYS_OPEN_DESC">					<option						value="0">JNo</option>					<option						value="1">JYes</option>				</field>				<field 					name="topbar_slide_img1" 					type="media"					label="Call Icon"					description="Here you can change Caller Icon"/>				<field					name="cell_number" 					type="text" 					label="Cell Number" 					description="Change Your Cell Number From Here" 					default="0697-000-662"/>				<field 					name="topbar_slide_img2" 					type="media"					label="Email Icon"					description="Change Your Email icon from here"/>				<field					name="mail_address" 					type="text" 					label="Email Address" 					description="Change Your Email Address From Here" 					default="mail@domain.com"/>										<field					name="social_caption" 					type="text" 					label="Layout 3" 					description="Here you can change your Social Title" 					default="Get In Touch"/>								<field					name="topbar_link1" 					type="text" 					label="Facebook Link" 					description="Facebook page link with http://" 					default="#"/>								<field					name="topbar_link2" 					type="text" 					label="Twitter Link" 					description="Twitter Profile link with http://" 					default="#"/>								<field					name="topbar_link3" 					type="text" 					label="Google Plus Link" 					description="Google Plus page link with http://" 					default="#"/>								<field					name="topbar_link4" 					type="text" 					label="Flicker Link" 					description="Flicker page link with http://" 					default="#"/>								<field					name="topbar_link5" 					type="text" 					label="Pinterest Link" 					description="Pinterest Profile link with http://" 					default="#"/>				<field type="asset" />			</fieldset>						<fieldset name="advanced">				<field name="moduleclass_sfx" type="text" default="" label="MODSFX" description="MODSFX_DESC" />				<field name="cache" type="list" default="1" label="COM_MODULES_FIELD_CACHING_LABEL" description="COM_MODULES_FIELD_CACHING_DESC">					<option value="1">JGLOBAL_USE_GLOBAL</option>					<option value="0">COM_MODULES_FIELD_VALUE_NOCACHING</option>				</field>				<field name="cache_time" type="text" default="900" label="COM_MODULES_FIELD_CACHE_TIME_LABEL" description="COM_MODULES_FIELD_CACHE_TIME_DESC" />				<field name="cachemode" type="hidden" default="itemid">					<option value="itemid"></option>				</field>			</fieldset>		</fields>	</config></extension>